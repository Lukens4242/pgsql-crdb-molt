# OpenSSL node configuration file
[ req ]
prompt=no
distinguished_name = distinguished_name
req_extensions = extensions

[ distinguished_name ]
organizationName = Cockroach

[ extensions ]
subjectAltName = @alt_names

[alt_names]
DNS.1 = host.docker.internal
DNS.2 = crdb_host
DNS.3 = localhost
IP.1 = 127.0.0.1
IP.2 = 172.27.0.102
IP.3 = 127.0.0.1